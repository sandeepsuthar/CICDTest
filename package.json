{
  "name": "Boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "i": "npm i --legacy-peer-deps && npx pod-install",
    "asset-link": "npx react-native-asset",
    "start": "react-native start",
    "start:clean": "cd android && ./gradlew clean && cd .. && npm cache clean --force && npm start -- --reset-cache",
    "clean": "npm cache clean --force && cd android && ./gradlew clean && cd ..",
    "android": "react-native run-android --active-arch-only --mode=devDebug --appIdSuffix=dev",
    "ios": "react-native run-ios --scheme \"Dev\"",
    "ios:device": "react-native run-ios --scheme \"Dev\" --device",
    "ios:dev": "react-native run-ios --scheme \"Dev\"",
    "ios:qat": "react-native run-ios --scheme \"QAT\"",
    "ios:uat": "react-native run-ios --scheme \"UAT\"",
    "ios:prod": "react-native run-ios --scheme \"Prod\"",
    "ios:dev:device": "react-native run-ios --scheme \"Dev\" --device",
    "ios:qat:device": "react-native run-ios --scheme \"QAT\" --device",
    "ios:uat:device": "react-native run-ios --scheme \"UAT\" --device",
    "ios:prod:device": "react-native run-ios --scheme \"Prod\" --device",
    "android:dev": "react-native run-android --active-arch-only --mode=devDebug --appIdSuffix=dev",
    "android:qat": "react-native run-android --active-arch-only --mode=qatDebug --appIdSuffix=qat",
    "android:uat": "react-native run-android --active-arch-only --mode=uatDebug --appIdSuffix=uat",
    "android:prod": "react-native run-android --active-arch-only --mode=prodDebug",
    "build:dev:release": "cd android && ./gradlew assembleDevRelease && cd ..",
    "build:qat:release": "cd android && ./gradlew assembleQatRelease && cd ..",
    "build:uat:release": "cd android && ./gradlew assembleUatRelease && cd ..",
    "build:prod:release": "cd android && ./gradlew assembleProdRelease && cd ..",
    "lint": "eslint .",
    "test": "jest",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@d11/react-native-fast-image": "^8.13.0",
    "@expo/react-native-action-sheet": "^4.1.0",
    "@gorhom/bottom-sheet": "^5.0.6",
    "@invertase/react-native-apple-authentication": "^2.4.0",
    "@miblanchard/react-native-slider": "^2.6.0",
    "@react-native-community/blur": "^4.4.1",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@react-native-firebase/app": "^21.7.2",
    "@react-native-firebase/auth": "^21.7.2",
    "@react-native-firebase/messaging": "^21.7.2",
    "@react-native-google-signin/google-signin": "^13.1.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/drawer": "^7.1.1",
    "@react-navigation/elements": "^2.2.5",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@react-navigation/stack": "^7.1.1",
    "@reduxjs/toolkit": "^2.5.1",
    "@twotalltotems/react-native-otp-input": "^1.3.11",
    "moment": "^2.30.1",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "react": "19.1.1",
    "react-native": "0.82.1",
    "react-native-config": "^1.5.3",
    "react-native-device-info": "^14.0.4",
    "react-native-gesture-handler": "2.29.1",
    "react-native-get-random-values": "^1.11.0",
    "react-native-image-picker": "^8.0.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-keyboard-manager": "^6.5.16-0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-localization": "^2.3.2",
    "react-native-push-notification": "^8.1.1",
    "react-native-reanimated": "^4.2.0",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^5.2.0",
    "react-native-screens": "^4.9.1",
    "react-native-sensitive-info": "^5.5.8",
    "react-native-snackbar": "^2.8.0",
    "react-native-step-indicator": "^1.0.3",
    "react-native-svg": "^15.15.1",
    "react-native-swiper-flatlist": "^3.2.5",
    "react-native-webview": "^13.13.2",
    "react-native-worklets": "^0.7.1",
    "react-navigation-shared-element": "^3.1.3",
    "react-redux": "^9.2.0",
    "redux-logger": "^3.0.6",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.1",
    "@react-native/eslint-config": "0.82.1",
    "@react-native/metro-config": "0.82.1",
    "@react-native/typescript-config": "0.82.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.1",
    "@types/react-native-get-random-values": "^1.8.2",
    "@types/react-native-push-notification": "^8.1.4",
    "@types/react-test-renderer": "^19.1.0",
    "@types/redux-logger": "^3.0.13",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.11.1",
    "jest": "^29.7.0",
    "prettier": "3.3.3",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "19.1.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
